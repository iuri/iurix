<script>
  <!--Venue vertical filters-->
    function venue_list_result_vertical(){
        //change width of venue image div start
	    $(".width_large").width(1000);
	        $(".image_large").height(1000);
		    $(".image_large").width(1000);
		        $(".panel-body").width(1000);
			    //change width of venue image div stop
			        $("#menu_align").val("v");
				    $("#venue_list").attr('class', 'col-lg-8');
				        $("#horizontal_menu").hide();
					    $("#horizontal_menu_one").hide();
					        $("#vertical_menu").show();
						    $("#map_div").hide();
						        $("#horizontal_filter_second").hide();
							    $("#horizontal_filter_blank").show();
							      }

  <!--Venue vertical filters-->

  check_key = "";
    document.addEventListener("keydown", KeyCheck);  //or however you are calling your method
      function KeyCheck(event) {
          var KeyID = event.keyCode;
	      switch(KeyID) {
	            case 8: //when backspace is pressed
		            check_key = "backspace";
			            //alert("backspace");
				            break;
					          case 46: //when delete is pressed
						          check_key = "backspace" ;
							          //alert("delete");
								          break;
									          default:
										          break;
											      }
											        }


  <!--search event type filters-->
    function select_event_type(val){
        //color change for vertical filters start
	    //event type start
	        //alert(val);
		    //width and height of image according to large view start
		        if($("#menu_align").val() == "h" || $("#menu_align").val() == "v"){
			      //change width of venue image div start
			            $(".width_large").width(10000);
				          $(".image_large").height(1000);
					        $(".image_large").width(1000);
						      $(".panel-body").width(1000);
							//change width of venue image div stop
							      } else if($("#menu_align").val() == ""){

      //change width of venue image div start
            $(".width_large").width(10000);
	          $(".image_large").height(1000);
		        $(".image_large").width(1000);
			      $(".panel-body").width(1000);
			            //change width of venue image div stop
				        }

    //width and height of image according to large view start
        if(val!='0'){
	      var val = val;
	            var eventType = val.split("_");
		          if(eventType[0] == 'vEventType' || eventType[0] == 'aEventType'){ //event type start
			          var cbs = $('.'+val+':checkbox:checked').length;
				          if(cbs != 0){
					            $('#'+val).css('background-color',pink);
						            } else {
							              $('#'+val).css('background-color','#fff');
								              }
									            } //event type stop

      //Venues Type start
            if(eventType[0] == 'vVenueType' || eventType[0] == 'neighbourhood'){
	            var vVenueType = $('.'+val+':checkbox:checked').length;
		            if(vVenueType != 0){
			              $('#'+val).css('background-color',pink);
				                $('#'+val).hover(function(e) {
						            $(this).css("background-color",e.type === "mouseenter"?"#2878fe":pink)
							            })
								          }else{
									          $('#'+val).css('background-color','#fff');
										          $('#'+val).hover(function(e) {
											            $(this).css("background-color",e.type === "mouseenter"?"#2878fe":"#fff")
												            })
													          }
														      }  //Venues Type stop
														        }

  //color change for vertical filters stop
    $("#loading_image").show();
      $("#venue_list_main").hide();
        var checkboxes;
	  var checkboxes_n;
	    var checkboxes_a;
	      var checkboxes_v;

  if($("#menu_align").val() == "h"){
    checkboxes = document.getElementsByName('option_sub[]');
      checkboxes_n = document.getElementsByName('option_n[]');
        checkboxes_a = document.getElementsByName('option_amenities[]');
	  checkboxes_v = document.getElementsByName('option_v[]');
	    } else if($("#menu_align").val() == "v"){
	      checkboxes = document.getElementsByName('option_sub1[]');
	        checkboxes_n = document.getElementsByName('option_n1[]');
		  checkboxes_a = document.getElementsByName('option_amenities1[]');
		    checkboxes_v = document.getElementsByName('option_v1[]');
		      } else if($("#menu_align").val() == ""){
		        checkboxes = document.getElementsByName('option_sub[]');
			  checkboxes_n = document.getElementsByName('option_n[]');
			    checkboxes_a = document.getElementsByName('option_amenities[]');
			      checkboxes_v = document.getElementsByName('option_v[]');
			        }

  var search_by_ratings = $("#search_by_ratings").val();
    var search_by_price = $("#search_by_price").val();
      var sorting_by = $("#sorting_by").val();
        var search_by_policies = $("#search_by_policies").val();
	  var drop_down_list_guest = $("#drop_down_list_guest").val();
	    var location = "";
	      if(check_key == "backspace"){
	          if($("#google_location_h").val() == ""){
		        $("#google_location_large").val("");
			      check_key = "";
			          } else if($("#google_location_large").val() == ""){
				        $("#google_location_h").val("");
					      check_key = "";
					          }
						    }


  if($("#google_location_h").val() != ""){
      location = $("#google_location_h").val();
        } else if($("#google_location_large").val() != "")
	    location = $("#google_location_large").val();

    var vals = "";
        var vals_n = "";
	    var vals_a = "";
	        var vals_v = "";
		    for (var i=0, n=checkboxes.length;i<n;i++) {
		          if (checkboxes[i].checked) {
			     //alert("sdf");
				vals += ","+checkboxes[i].value;
				      }
				          }
					      for (var i=0, n=checkboxes_a.length;i<n;i++) {
					            if (checkboxes_a[i].checked) {
						       vals_a += ","+checkboxes_a[i].value;
						             }
							         }

    for (var i=0, n=checkboxes_n.length;i<n;i++) {
          if (checkboxes_n[i].checked) {
	     vals_n += ","+checkboxes_n[i].value;
	           }
		       }

    for (var i=0, n=checkboxes_v.length;i<n;i++) {
          if (checkboxes_v[i].checked) {
	          vals_v += ","+checkboxes_v[i].value;
		        }
			    }

    var keyword = $("#keyword").val();


    $.ajax({
          type:"post",
	        data: "type=v&option_sub="+vals+"&option_n="+vals_n+"&option_a="+vals_a+"&option_v="+vals_v+"&google_location="+location+"&keyword="+keyword+"&drop_down_list_ratings="+search_by_ratings+"&search_by_price="+search_by_price+"&search_by_policies="+search_by_policies+"&drop_down_list_guest="+drop_down_list_guest+"&sorting_by="+sorting_by,
		      url:"/welcome/search_items_ajax",
		            success:function(data){
			              console.log(data);
				                $("#loading_image").hide();
							  $("#venue_list_main").show();
							            $("#venue_list_main").html(data);
								              init_map_on_filter_selection();
										  $('html,body').animate({
											    scrollTop: $("#header").offset().top
											    	         }, 'slow');
													         },
														         error: function(xhr, textStatus, thrownError, data) {
															           alert("Error1: " + thrownError);
																             alert("XHR: " + xhr.responseText);
																	             }

      });
          }

    <!--search event type filters-->
        $("#google_location_h").keypress(function(e) {
	      if(e.which == 13) {
	              select_event_type('0');
		            }
			        });
				    $("#google_location_large").keypress(function(e) {
				          if(e.which == 13) {
					          select_event_type('0');
						        }
							    });

</script>






     <script>

var colors = new Array(
  [237,18,95],
    [247,128,43],
      [237,18,95],
        [247,128,43],
	  [237,18,95],
	    [247,128,43]);

var step = 0;
//color table indices for:
// current color left
// next color left
// current color right
// next color right
var colorIndices = [0,1,2,3];

//transition speed
var gradientSpeed = 0.002;

function updateGradient()
{

  if ( $===undefined ) return;

var c0_0 = colors[colorIndices[0]];
var c0_1 = colors[colorIndices[1]];
var c1_0 = colors[colorIndices[2]];
var c1_1 = colors[colorIndices[3]];

var istep = 1 - step;
var r1 = Math.round(istep * c0_0[0] + step * c0_1[0]);
var g1 = Math.round(istep * c0_0[1] + step * c0_1[1]);
var b1 = Math.round(istep * c0_0[2] + step * c0_1[2]);
var color1 = "rgb("+r1+","+g1+","+b1+")";

var r2 = Math.round(istep * c1_0[0] + step * c1_1[0]);
var g2 = Math.round(istep * c1_0[1] + step * c1_1[1]);
var b2 = Math.round(istep * c1_0[2] + step * c1_1[2]);
var color2 = "rgb("+r2+","+g2+","+b2+")";

 $('#horizontal_menu').css({
    background: "-webkit-gradient(linear, left top, right top, from("+color1+"), to("+color2+"))"}).css({
        background: "-moz-linear-gradient(left, "+color1+" 0%, "+color2+" 100%)"});

  step += gradientSpeed;
    if ( step >= 1 )
      {
          step %= 1;
	      colorIndices[0] = colorIndices[1];
	          colorIndices[2] = colorIndices[3];

    //pick two new target color indices
        //do not pick the same as the current one
	    colorIndices[1] = ( colorIndices[1] + Math.floor( 1 + Math.random() * (colors.length - 1))) % colors.length;
	        colorIndices[3] = ( colorIndices[3] + Math.floor( 1 + Math.random() * (colors.length - 1))) % colors.length;

  }
  }

setInterval(updateGradient,10);


    </script>
