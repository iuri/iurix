ad_page_contract {}



set url "https://www.ecb.europa.eu/stats/policy_and_exchange_rates/euro_reference_exchange_rates/html/brl.xml"


    
    set result [ns_http run $url]
    #    ns_log Notice "$xml"

    set status [dict get $result status]
    ns_log Notice "STATUS $status"

    
    set xml [dict get $result body]
    ns_log Notice "XML \n $xml"
    
    set doc [dom parse $xml]

    ns_log Notice "DOC \n $doc"
    
    set root [$doc documentElement]
