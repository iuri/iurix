<p>#chat.Are_you_sure_you_want_to_delete#?</p> 
<multiple name="rooms">
@rooms.room_id@ -  @rooms.pretty_name@<br>
</multiple>

<div>
  @hidden_vars;noquote@
  <input type=submit name=submit.x value=#acs-kernel.common_Yes#>
  <input type=submit name=cancel.x value=#acs-kernel.common_No#>
</div>
</form>

